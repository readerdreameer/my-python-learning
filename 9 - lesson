mehmonlar = ["Anvar","Zokir","Nodir","G'ofur"]
for mehmon in mehmonlar:
 print(f"Hurmatli {mehmon} sizni ertaga nahorgi oshga taklif qilamiz")
 print("Hurmat biland Miraxmedovlar oilasi")
 print(mehmon)
print(mehmonlar)
sonlar = list(range(1,11))
or son in sonlar :
 print(f"{son} ning kvadrati {son**2} ga teng")
sonlar_kvadrati = []
for son in sonlar :
 sonlar_kvadrati.append(son**2)
print(sonlar_kvadrati)
friends = []
print("5 ta eng yaqin do'stingiz kim?")
for f in range(1,6):
 friends.append(input(f"{f} - do'stingizni ismini kiriting\n-")) 
ismlar = ["Abror","Doston","Bekzod","Zokir","Nodir"]
for ism in ismlar :
    print("Assalomu aleykum",ism)
print(f"Kod {len(ismlar)} marta takrorlandi")
sonlar = list(range(11,100,2))
for son in sonlar:
 print(son**3)
kinolar = []
print("5 ta yoqtirgan kinoyingizning nomini kiriting")
for kino in range(1,6) : 
 kinolar.append(input(f"{kino} - kinoning nomi:"))
people = int(input("Bugun necha kishi bilan suhbat qurdingiz?\n:"))
names = []
for n in range(people):
 names.append(input(f"{n+1} - suhbat qurgan odamingizni ismini kiriting :"))
print(names)    








